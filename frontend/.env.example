# =====================================
# RAG ASISTENTE FRONTEND - CONFIGURACIÓN
# =====================================

# URL del backend API
REACT_APP_API_URL=http://localhost:8000

# Puerto del frontend (opcional, por defecto 3000)
PORT=3000

# Configuraciones de desarrollo
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# Configuraciones de autenticación
REACT_APP_JWT_TOKEN_NAME=access_token
REACT_APP_JWT_REFRESH_NAME=refresh_token

# Configuraciones de la aplicación
REACT_APP_NAME=RAG Asistente
REACT_APP_VERSION=1.0.0